#include <graphics.h>
#include <conio.h>


void drawshape() {
	setlinecolor(RGB(21, 102, 230));
	setlinestyle(PS_SOLID, 3);
	setfillcolor(RGB(255, 250, 174));
	for (int j = 10; j < getwidth() - 10; j += 40) {

		for (int i = 10; i < getwidth() - 10; i += 40) {
			Sleep(20);
			fillroundrect(i, j, i + 40, j + 40, 10, 10);
			if ((i ==50 && j == 10) || (i ==90 && j == 10) || (i ==130 && j == 10)||
				(i == 50 && j == 90) || (i == 90 && j == 90) || (i == 130 && j == 90) ||
				(i == 50 && j == 170) || (i == 90 && j == 170) || (i == 130 && j == 170) ||
				(i == 130 && j == 50) || (i == 50 && j == 130) || 
				
				(i == 210 && j == 10) || (i == 250 && j == 10) || (i == 290 && j == 10) ||
				(i == 210 && j == 90) || (i == 290 && j == 50) || (i == 290 && j == 90) ||
				(i == 210 && j == 170) || (i == 250 && j == 170) || (i == 290 && j == 170) ||
				(i == 210 && j == 50) || (i == 210 && j == 130) || (i == 290 && j == 130)||


				(i == 50 && j == 250) || (i == 90 && j == 250) || (i == 130 && j == 250) ||
				(i == 50 && j == 330) || (i == 90 && j == 330) || (i == 130 && j == 330) ||
				(i == 50 && j == 410) || (i == 90 && j ==410) || (i == 130 && j == 410) ||
				(i == 130 && j == 290) || (i == 50 && j == 370) ||

				(i == 210 && j == 250) || (i == 250 && j == 250) || (i == 290 && j == 250) ||
				(i == 210 && j == 330) || (i == 250 && j == 330) || (i == 290 && j == 330) ||
				(i == 210 && j == 410) || (i == 250 && j == 410) || (i == 290 && j == 410) ||
				(i == 210 && j == 290) || (i == 210 && j == 370) || (i == 290 && j == 370)

				
				) {
			setfillcolor(RGB(221, 211, 255));
			fillroundrect(i, j, i + 40, j + 40, 10, 10);
			setfillcolor(RGB(255, 250, 174));
			}
		}
	}
	
	
	
}

int main()
{
	initgraph(820, 820);
	setbkcolor(RGB(139, 196, 171));
	cleardevice();
	drawshape();
	_getch();//卡住不立即退出
	closegraph();
	return 0;
}
